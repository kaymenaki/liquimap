<?xml version="1.0" encoding="ASCII"?>
<xmi:XMI xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="liquimap">
  <Blueprint name="Shop shoes">
    <stages name="Awareness" colour="rgb(0, 80, 93)">
      <steps name="Wants to buy new shoes">
        <touchpoints name="Ads" departments="marketing"/>
        <touchpoints name="e-Shop" departments="marketing eshop"/>
      </steps>
      <steps name="Clicks ad on landing page">
        <touchpoints name="Blog" departments="marketing"/>
      </steps>
    </stages>
    <stages name="Consideration" colour="rgb(78, 132, 154)">
      <steps name="Visits e-Shop">
        <touchpoints name="Landing Page" departments="marketing design"/>
        <touchpoints name="Form" departments="marketing"/>
        <touchpoints name="Product Page" departments="design"/>
      </steps>
    </stages>
    <departments id="marketing" name="Marketing/Online Marketing"/>
    <departments id="eshop" name="e-Shop Management"/>
    <departments id="design" name="Design"/>
  </Blueprint>
  <ExperienceMap name="Shop shoes">
    <nodes xsi:type="Action" id="a1" name="Arrives at langing page" outgoing="/1/@transitions.0" emotion="3" mood="Mixed feelings" image="images/image1.png" comments="N/A">
      <pains>Page looks terrible</pains>
      <gains>Page loads fast</gains>
      <opportunities>Sales!</opportunities>
    </nodes>
    <nodes xsi:type="Decision" id="d1" name="Man or woman?" outgoing="t1 t2" incoming="/1/@transitions.0"/>
    <nodes xsi:type="Action" id="a2" name="Visit mens page" outgoing="/1/@transitions.3" incoming="t1" image="images/image1.png"/>
    <nodes xsi:type="Action" id="a3" name="Visit womens page" outgoing="/1/@transitions.4" incoming="t2" image="images/image1.png"/>
    <nodes xsi:type="Action" id="a4" name="Check out" incoming="/1/@transitions.3 /1/@transitions.4" image="images/image1.png"/>
    <transitions from="a1" to="d1"/>
    <transitions id="t1" name="man" from="d1" to="a2"/>
    <transitions id="t2" name="woman" from="d1" to="a3"/>
    <transitions from="a2" to="a4"/>
    <transitions from="a3" to="a4"/>
    <journeys name="Man shops shoes" expectations="To buy functional shoes" choices="t1"/>
    <journeys name="Woman shops shoes" expectations="To buy expensive shoes" choices="t2"/>
  </ExperienceMap>
</xmi:XMI>
