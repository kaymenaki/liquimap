<?xml version="1.0" encoding="ASCII"?>
<xmi:XMI xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="liquimap">
  <Blueprint name="Shop shoes">
    <stages name="Research" colour="rgb(4, 65, 37)">
      <steps name="Wants to buy new shoes">
        <touchpoints name="Ads" departments="marketing"/>
        <touchpoints name="e-Shop" departments="marketing eshop"/>
      </steps>
      <steps name="Clicks ad on landing page">
        <touchpoints name="Blog" departments="marketing product"/>
      </steps>
    </stages>
    <stages name="Awareness" colour="rgb(13, 107, 189)">
      <steps name="Wants to buy new shoes">
        <touchpoints name="Ads" departments="marketing"/>
        <touchpoints name="e-Shop" departments="marketing eshop"/>
      </steps>
      <steps name="Clicks ad on landing page">
        <touchpoints name="Blog" departments="marketing product"/>
      </steps>
    </stages>
    <stages name="Consideration" colour="rgb(16, 135, 236)">
      <steps name="Visits e-Shop">
        <touchpoints name="Landing Page" departments="marketing design product"/>
        <touchpoints name="Form" departments="marketing legal"/>
        <touchpoints name="Product Page" departments="design"/>
      </steps>
    </stages>
    <stages name="Acquisition" colour="rgb(191, 89, 0)">
      <steps name="Check out">
        <touchpoints name="Login" departments="design eshop customer legal"/>
        <touchpoints name="Product Page" departments="design"/>
      </steps>
      <steps name="Thank you page">
        <touchpoints name="Thank you page" departments="marketing design eshop customer legal payment"/>
      </steps>
    </stages>
    <stages name="Loyalty" colour="rgb(230, 107, 0)">
      <steps name="Vouchers">
        <touchpoints name="Landing Page" departments="marketing design eshop customer"/>
        <touchpoints name="Form" departments="eshop customer legal"/>
        <touchpoints name="Newsletter" departments="marketing design customer"/>
      </steps>
    </stages>
    <departments id="marketing" name="Marketing/Online Marketing" colour="rgb(208, 208, 208)"/>
    <departments id="eshop" name="e-Shop Management"/>
    <departments id="design" name="Design"/>
    <departments id="customer" name="Customer Management"/>
    <departments id="legal" name="Legal"/>
    <departments id="payment" name="Payment services"/>
    <departments id="product" name="Product Management"/>
  </Blueprint>
  <ExperienceMap name="Shop shoes">
    <nodes xsi:type="Action" id="a1" name="Arrives at langing page" outgoing="/1/@transitions.0" emotion="3" mood="Mixed feelings" image="images/image1.png" comments="N/A" colour="rgb(13, 107, 189)">
      <pains>Page looks terrible</pains>
      <gains>Page loads fast</gains>
      <opportunities>Sales!</opportunities>
    </nodes>
    <nodes xsi:type="Decision" id="d1" name="Man or woman?" outgoing="t1 t2" incoming="/1/@transitions.0"/>
    <nodes xsi:type="Action" id="a2" name="Visit mens page" outgoing="/1/@transitions.3" incoming="t1" image="images/image1.png" colour="rgb(13, 107, 189)"/>
    <nodes xsi:type="Action" id="a3" name="Visit womens page" outgoing="/1/@transitions.4" incoming="t2" image="images/image1.png" colour="rgb(13, 107, 189)"/>
    <nodes xsi:type="Action" id="a4" name="Check out" incoming="/1/@transitions.3 /1/@transitions.4" image="images/image1.png" colour="rgb(13, 107, 189)"/>
    <transitions from="a1" to="d1"/>
    <transitions id="t1" name="man" from="d1" to="a2"/>
    <transitions id="t2" name="woman" from="d1" to="a3"/>
    <transitions from="a2" to="a4"/>
    <transitions from="a3" to="a4"/>
    <journeys name="Man shops shoes" expectations="To buy functional shoes" choices="t1"/>
    <journeys name="Woman shops shoes" expectations="To buy expensive shoes" choices="t2"/>
  </ExperienceMap>
</xmi:XMI>
